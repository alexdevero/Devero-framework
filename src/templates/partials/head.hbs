<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="{{{ meta.basic.viewport }}}">
<meta name="handheldFriendly" content="{{{ meta.basic.handheldFriendly }}}">

<!--
  Allows control over where resources are loaded from.
  Place as early in the <head> as possible, as the tag
  only applies to resources that are declared after it.
-->
<meta http-equiv="Content-Security-Policy" content="{{{ meta.basic.content-security-policy }}}">

<!-- Meta tags mostly for SEO -->
<meta name="description" content="{{{ meta.basic.description }}}"><!-- description of the website -->
<meta name="subject" content="{{{ meta.basic.subject }}}">
<meta name="language" content="{{{ meta.basic.language }}}"><!-- Language of the website -->
<meta name="robots" content="{{{ meta.basic.robots }}}">
<meta name="googlebot" content="{{{ meta.basic.googlebot }}}">
<meta name="classification" content="{{{ meta.basic.classification }}}"><!-- Classification of the website -->
<meta name="url" content="{{{ meta.basic.url }}}"><!-- URL of the website -->
<meta name="identifier-URL" content="{{{ meta.basic.identifier-url }}}"><!-- URL of the website -->
<meta name="coverage" content="{{{ meta.basic.coverage }}}">
<meta name="distribution" content="{{{ meta.basic.distribution }}}"><!-- Degree of distribution of your webpage: global, local, IU -->
<meta name="rating" content="{{{ meta.basic.rating }}}"><!-- Rating of the website: general, mature, restricted, 14 years, safe for kids -->
<meta name="author" content="{{{ meta.basic.author }}}"><!-- You name -->
<meta name="copyright" content="{{{ meta.basic.copyright }}}">

<!-- Verify website ownership -->
<meta name="google-site-verification" content="verification_token"><!-- Google Search Console -->
<meta name="yandex-verification" content="verification_token"><!-- Yandex Webmasters -->
<meta name="msvalidate.01" content="verification_token"><!-- Bing Webmaster Center -->
<meta name="alexaVerifyID" content="verification_token"><!-- Alexa Console -->
<meta name="p:domain_verify" content="code_from_pinterest"><!-- Pinterest Console-->
<meta name="norton-safeweb-site-verification" content="norton_code"><!-- Norton Safe Web -->

<!-- Favicons -->
<link rel="apple-touch-icon" sizes="180x180" href="{{{ favicon.apple-touch-icon }}}">
<link rel="icon" type="image/png" href="{{{ favicon.icon-32 }}}" sizes="32x32">
<link rel="icon" type="image/png" href="{{{ favicon.icon-16 }}}" sizes="16x16">
<link rel="manifest" href="{{{ favicon.manifest }}}">
<link rel="mask-icon" href="{{{ favicon.mask-icon }}}" color="#5bbad5">
<link rel="shortcut icon" href="{{{ favicon.shortcut-icon }}}">
<meta name="msapplication-config" content="{{{ favicon.msapplication-config }}}">
<meta name="theme-color" content="{{{ favicon.theme-color }}}">

<!-- Facebook OpenGraph tags -->
<meta property="og:title" content="{{{ socialCards.card-facebook.title }}}"><!-- Title of the website -->
<meta property="og:type" content="{{{ socialCards.card-facebook.type }}}"><!-- Type of the Facebook OpenGraph tag -->
<meta property="og:description" content="{{{ socialCards.card-facebook.description }}}"><!-- Description of the website -->
<meta property="og:image" content="{{{ socialCards.card-facebook.image }}}"><!-- URL Your photo -->
<meta property="og:url" content="{{{ socialCards.card-facebook.url }}}"><!-- Url of website -->
<meta property="og:locale" content="{{{ socialCards.card-facebook.locale }}}"><!-- Default language -->
<meta property="og:site_name" content="{{{ socialCards.card-facebook.site-name }}}"><!-- Name of the website -->
<meta property="article:author" content="{{{ socialCards.card-facebook.author }}}"><!-- Name of the author -->

<!-- Twitter card tags -->
<meta name="twitter:card" content="{{{ socialCards.card-twitter.card }}}"><!-- Type of Twitter card -->
<meta name="twitter:site" content="{{{ socialCards.card-twitter.site }}}"><!-- Your Twitter username -->
<meta name="twitter:creator" content="{{{ socialCards.card-twitter.creator }}}"><!-- Your Twitter username -->
<meta name="twitter:url" content="{{{ socialCards.card-twitter.url }}}"><!-- URL to your website -->
<meta name="twitter:title" content="{{{ socialCards.card-twitter.title }}}"><!-- Title of the website -->
<meta name="twitter:description" content="{{{ socialCards.card-twitter.description }}}"><!-- Description of the website -->
<meta name="twitter:image" content="{{{ socialCards.card-twitter.image }}}"><!-- URL to image of the website -->

